        <icon height="87" src="images/ios/icon/icon-small@3x.png" width="87" />
        <icon height="1024" src="images/ios/icon/icon-1024.png" width="1024" />
        <splash height="480" src="images/ios/splash/Default~iphone.png" width="320" />
        <splash height="960" src="images/ios/splash/Default@2x~iphone.png" width="640" />
        <splash height="1024" src="images/ios/splash/Default-Portrait~ipad.png" width="768" />
