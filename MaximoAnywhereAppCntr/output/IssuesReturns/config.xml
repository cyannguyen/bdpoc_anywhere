        <splash height="768" src="images/ios/splash/Default-Landscape~ipad.png" width="1024" />
        <splash height="1536" src="images/ios/splash/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="1136" src="images/ios/splash/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="images/ios/splash/Default-667h.png" width="750" />
        <splash height="2208" src="images/ios/splash/Default-736h.png" width="1242" />
        <splash height="2436" src="images/ios/splash/Default-2436h.png" width="1125" />
        <splash height="1242" src="images/ios/splash/Default-Landscape-736h.png" width="2208" />
        <splash height="1125" src="images/ios/splash/Default-Landscape-2436h.png" width="2436" />
    </platform>
    <platform name="windows">
        <icon src="images/windows/icon/Square71x71Logo.png" target="Square71x71Logo" />
        <icon src="images/windows/icon/Square150x150Logo.png" target="Square150x150Logo" />
        <icon src="images/windows/splash/Wide310x150Logo.png" target="Wide310x150Logo" />
        <icon src="images/windows/icon/Square310x310Logo.png" target="Square310x310Logo" />
        <icon src="images/windows/icon/Square44x44Logo.png" target="Square44x44Logo" />
        <icon src="images/windows/icon/StoreLogo.png" target="StoreLogo" />
        <icon src="images/windows/icon/Square44x44Logo.png" target="BadgeLogo" />
        <splash src="images/windows/splash/SplashScreen.png" target="splashscreen" />
    </platform>
    <mfp:android>
        <mfp:sdkChecksum>0</mfp:sdkChecksum>
        <mfp:appChecksum>0</mfp:appChecksum>
        <mfp:security>
            <mfp:testWebResourcesChecksum enabled="false" ignoreFileExtensions="png, jpg, jpeg, gif, mp4, mp3" />
        </mfp:security>
    </mfp:android>
    <mfp:ios>
        <mfp:sdkChecksum>4225698395</mfp:sdkChecksum>
        <mfp:appChecksum>0</mfp:appChecksum>
        <mfp:security>
            <mfp:testWebResourcesChecksum enabled="false" ignoreFileExtensions="png, jpg, jpeg, gif, mp4, mp3" />
        </mfp:security>
    </mfp:ios>
    <mfp:windows>
        <mfp:windows8>
            <mfp:sdkChecksum>0</mfp:sdkChecksum>
            <mfp:security>
                <mfp:testWebResourcesChecksum enabled="false" ignoreFileExtensions="png, jpg, jpeg, gif, mp4, mp3" />
            </mfp:security>
        </mfp:windows8>
        <mfp:windowsphone8>
            <mfp:sdkChecksum>0</mfp:sdkChecksum>
            <mfp:security>
                <mfp:testWebResourcesChecksum enabled="false" ignoreFileExtensions="png, jpg, jpeg, gif, mp4, mp3" />
            </mfp:security>
        </mfp:windowsphone8>
        <mfp:windows10>
