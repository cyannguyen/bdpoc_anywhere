<?xml version='1.0' encoding='utf-8'?>
<widget id="com.avenue.maximoanywhere.IssuesReturns" version="7.6.4" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:mfp="http://www.ibm.com/mobilefirst/cordova-plugin-mfp">
    <name>IBMItemMove</name>
    <description>Issues Returns</description>
    <author email="viet.nguyen@avenue-net.com" href="http://mycompany.com">Viet Nguyen</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="hooks/android_clearText.js" type="after_prepare" />
    <hook src="hooks/reset_mfpSplash.js" type="before_compile" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="images/android/icon/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="images/android/icon/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="images/android/icon/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="images/android/icon/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="images/android/icon/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="images/android/icon/drawable-xxxhdpi-icon.png" />
        <splash density="land-hdpi" src="images/android/splash/drawable-land-hdpi-screen.png" />
        <splash density="land-ldpi" src="images/android/splash/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="images/android/splash/drawable-land-mdpi-screen.png" />
        <splash density="land-xhdpi" src="images/android/splash/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="images/android/splash/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="images/android/splash/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-hdpi" src="images/android/splash/drawable-port-hdpi-screen.png" />
        <splash density="port-ldpi" src="images/android/splash/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="images/android/splash/drawable-port-mdpi-screen.png" />
        <splash density="port-xhdpi" src="images/android/splash/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="images/android/splash/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="images/android/splash/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <edit-config file="*-Info.plist" mode="merge" target="NSBluetoothPeripheralUsageDescription">
            <string>Scans bar codes.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
            <string>Captures photos for attachments or scans a bar code.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationAlwaysAndWhenInUseUsageDescription">
            <string>Your location is used to plan and route work activities.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationAlwaysUsageDescription">
            <string>our location is used to plan and route work activities.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationUsageDescription">
            <string>Your location is used to plan and route work activities.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
            <string>Your location is used to plan and route work activities.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryAddUsageDescription">
            <string>Captures photos for attachments or scans a bar code.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
            <string>Captures photos for attachments.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="UIBackgroundModes">
            <array>
                <string>fetch</string>
                <string>location</string>
                <string>remote-notification</string>
            </array>
        </edit-config>
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="images/ios/icon/icon-60@3x.png" width="180" />
        <icon height="120" src="images/ios/icon/icon-60@2x.png" width="120" />
        <icon height="76" src="images/ios/icon/icon-76.png" width="76" />
        <icon height="152" src="images/ios/icon/icon-76@2x.png" width="152" />
        <icon height="167" src="images/ios/icon/icon-83.5@2x.png" width="167" />
        <icon height="40" src="images/ios/icon/icon-40.png" width="40" />
        <icon height="80" src="images/ios/icon/icon-40@2x.png" width="80" />
        <icon height="29" src="images/ios/icon/icon-small.png" width="29" />
